{
  "name": "sniffhunt-scraper-mcp-server",
  "version": "1.0.0",
  "description": "Advanced Model Context Protocol server for web scraping, content extraction, data harvesting, and website content mining with SniffHunt scraper",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "sniffhunt-scraper-mcp-server": "dist/index.js"
  },
  "engines": {
    "bun": ">=1.2.15"
  },
  "scripts": {
    "build": "bun build src/index.js --target bun --format esm --outfile dist/index.js --external playwright-core",
    "dev": "bun --watch src/index.js",
    "setup": "bun setup.js"
  },
  "keywords": [
    "mcp",
    "web-scraper",
    "scraping",
    "content-extraction",
    "data-harvesting",
    "website-extraction",
    "content-grabbing",
    "data-mining",
    "web-crawling",
    "html-extraction",
    "content-collection",
    "website-data",
    "sniffhunt",
    "model-context-protocol",
    "extract",
    "scrape",
    "grab",
    "fetch",
    "pull",
    "harvest",
    "mine",
    "collect",
    "gather",
    "retrieve"
  ],
  "author": "SniffHunt Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.2",
    "dotenv": "^17.2.3",
    "playwright-core": "^1.56.1"
  },
  "devDependencies": {
    "@types/bun": "^1.3.1"
  }
}
