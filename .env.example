# Environment Variables for SniffHunt Scraper
# Copy this file to .env and add your actual values

# Google Gemini API Key (Required for AI processing)
# Get your API key from: https://makersuite.google.com/app/apikey
GOOGLE_GEMINI_KEY=your_google_gemini_api_key_here

# Alternative Gemini API Keys (Optional - for load balancing)
GOOGLE_GEMINI_KEY1=your_alternative_key_1
GOOGLE_GEMINI_KEY2=your_alternative_key_2
GOOGLE_GEMINI_KEY3=your_alternative_key_3

# Scraper Configuration (Optional)
MAX_RETRY_COUNT=2
RETRY_DELAY=1000
PAGE_TIMEOUT=10000

# Server Configuration (Optional)
PORT=8080
CORS_ORIGIN=*
